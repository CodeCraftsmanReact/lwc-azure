{
  "routes": [
    { "route": "/admin/*", "allowedRoles": ["admin"] },
    { "route": "/dashboard/*", "allowedRoles": ["authenticated"] },
    { "route": "/*", "allowedRoles": ["anonymous", "authenticated"] }
  ],
  "auth": {
    "rolesSource": "/api/getRolesForUsers",
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/9384d563-29cc-4c75-b11d-e9fce6b78c6e",
          "clientIdSettingName": "AZURE_CLIENT_ID",
          "clientSecretSettingName": "AZURE_CLIENT_SECRET_APP_SETTING_NAME"
        }
      },
      "google": {
        "registration": {
          "clientIdSettingName": "GOOGLE_CLIENT_ID",
          "clientSecretSettingName": "GOOGLE_CLIENT_SECRET_APP_SETTING_NAME"
        }
      }
    }
  }
}
